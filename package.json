{
  "name": "pug-static-template",
  "version": "7.0.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "npm run dev:cache",
    "dev:default": "npx gulp",
    "dev:cache": "NODE_CACHE=true npm run dev:default",
    "dev:clean": "npm run dev:default",
    "dev:php": "NODE_PHP=true npm run dev:cache",
    "dev:example": "NODE_EXAMPLE=true npm run dev:cache",
    "dev:example:clean": "NODE_EXAMPLE=true npm run dev:clean",
    "build": "npm run build:default",
    "build:default": "NODE_ENV=production npx gulp build",
    "build:cache": "NODE_CACHE=true npm run build:default",
    "build:php": "NODE_PHP=true npm run build",
    "build:example": "NODE_EXAMPLE=true npm run build",
    "wp": "NODE_ENV=production npx gulp buildWp",
    "tinypng": "NODE_TINYPNG=true npx gulp tinypng",
    "deploy": "npx gulp deploy",
    "lint": "npm run stylelint && npm run eslint",
    "lint:fix": "npm run stylelint:fix && npm run eslint:fix",
    "eslint": "eslint . --ext .js",
    "eslint:fix": "eslint --fix . --ext .js",
    "stylelint": "stylelint \"./src/**/*.scss\" -s scss",
    "stylelint:fix": "stylelint --fix \"./src/**/*.scss\" -s scss",
    "doc": "npm run doc:generate && open-cli sassdoc/index.html && open-cli jsdoc/index.html",
    "doc:generate": "sassdoc src/modules/css && jsdoc -c jsdocrc.json"
  },
